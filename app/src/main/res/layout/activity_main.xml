<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/RLTopLayout"
                    android:layout_width="match_parent"
                    android:layout_height="350dp"
                    android:background="@drawable/rectangle_blue"
                    android:visibility="visible">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="253dp"
                        android:layout_marginBottom="120dp">


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:layout_marginStart="15dp"
                            android:layout_marginEnd="15dp"
                            android:background="@drawable/rectangle_white"
                            android:paddingTop="40dp"
                            android:paddingBottom="40dp">

                            <ImageView
                                android:id="@+id/IMIcon"
                                android:layout_width="200dp"
                                android:layout_height="80dp"
                                android:layout_centerHorizontal="true"
                                android:src="@drawable/icon_tr" />

                            <TextView
                                android:id="@+id/TVTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/IMIcon"
                                android:layout_centerHorizontal="true"
                                android:text="@string/app_name"
                                android:textColor="@color/colorPrimaryDark"
                                android:textSize="@dimen/home_app_title"
                                android:textStyle="bold" />

                        </RelativeLayout>

                    </RelativeLayout>

                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/lnr_main"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/RLTopLayout"
                    android:layout_marginTop="-120dp"
                    android:orientation="vertical"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:paddingBottom="10dp">


                    <RelativeLayout
                        android:id="@+id/RLTopMain"
                        android:layout_width="match_parent"
                        android:layout_height="476dp"
                        android:layout_marginStart="5dp"
                        android:layout_marginEnd="5dp"
                        android:background="@drawable/rectangle_white"
                        android:elevation="3dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:weightSum="3">

                                <RelativeLayout
                                    android:id="@+id/rvRio"
                                    android:layout_width="match_parent"
                                    android:layout_height="160dp"
                                    android:layout_weight="1">

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true"
                                        android:gravity="center"
                                        android:orientation="vertical">

                                        <ImageView
                                            android:layout_width="50dp"
                                            android:layout_height="50dp"
                                            android:padding="0dp"
                                            android:src="@drawable/cafe" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:layout_marginTop="5dp"
                                            android:gravity="center"
                                            android:text="@string/Rio"
                                            android:textColor="@color/colorPrimaryDark"
                                            android:textSize="@dimen/home_item_txt_size" />
                                    </LinearLayout>
                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/getcard"
                                    android:layout_width="match_parent"
                                    android:layout_height="160dp"
                                    android:layout_weight="1">

                                    <LinearLayout
                                        android:layout_width="81dp"
                                        android:layout_height="116dp"
                                        android:layout_centerInParent="true"
                                        android:gravity="center"
                                        android:orientation="vertical"><![CDATA[

                                        " />



                                        ]]>

                                        <ImageView
                                            android:layout_width="83dp"
                                            android:layout_height="wrap_content"
                                            android:padding="0dp"
                                            android:src="@drawable/nnnn" />

                                        <TextView
                                            android:layout_width="72dp"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="5dp"
                                            android:gravity="center"
                                            android:text="@string/red_m"
                                            android:textColor="@color/colorPrimaryDark"
                                            android:textSize="@dimen/home_item_txt_size" />
                                    </LinearLayout>
                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/alreef_rv"
                                    android:layout_width="match_parent"
                                    android:layout_height="160dp"
                                    android:layout_weight="1">

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true"
                                        android:gravity="center"
                                        android:orientation="vertical">

                                        <ImageView
                                            android:layout_width="50dp"
                                            android:layout_height="50dp"
                                            android:padding="0dp"
                                            android:src="@drawable/reef" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="5dp"
                                            android:gravity="center"
                                            android:text="@string/reef"
                                            android:textColor="@color/colorPrimaryDark"
                                            android:textSize="@dimen/home_item_txt_size" />
                                    </LinearLayout>
                                </RelativeLayout>


                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:weightSum="3">

                                <RelativeLayout
                                    android:id="@+id/rvprice"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true"
                                        android:gravity="center"
                                        android:orientation="vertical">

                                        <ImageView
                                            android:layout_width="80dp"
                                            android:layout_height="43dp"
                                            android:padding="0dp"
                                            android:src="@drawable/ksho" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:layout_marginTop="5dp"
                                            android:gravity="center"
                                            android:text="@string/kasho"
                                            android:textColor="@color/colorPrimaryDark"
                                            android:textSize="@dimen/home_item_txt_size" />
                                    </LinearLayout>
                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/rvShareChat"
                                    android:layout_width="match_parent"
                                    android:layout_height="160dp"
                                    android:layout_weight="1"
                                    android:visibility="visible">

                                    <LinearLayout
                                        android:layout_width="109dp"
                                        android:layout_height="86dp"
                                        android:layout_centerInParent="true"
                                        android:gravity="center"
                                        android:orientation="vertical">

                                        <ImageView
                                            android:layout_width="87dp"
                                            android:layout_height="50dp"
                                            android:padding="0dp"
                                            android:src="@drawable/lazmek" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="5dp"
                                            android:gravity="center"
                                            android:text="@string/lazmek_m"
                                            android:textColor="@color/colorPrimaryDark"
                                            android:textSize="@dimen/home_item_txt_size" />
                                    </LinearLayout>
                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/rvIstaitoin"
                                    android:layout_width="match_parent"
                                    android:layout_height="160dp"
                                    android:layout_weight="1">

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true"
                                        android:gravity="center"
                                        android:orientation="vertical">

                                        <ImageView
                                            android:layout_width="106dp"
                                            android:layout_height="50dp"
                                            android:padding="0dp"
                                            android:src="@drawable/icon_tr" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="5dp"
                                            android:gravity="center"
                                            android:text="@string/stations"
                                            android:textColor="@color/colorPrimaryDark"
                                            android:textSize="@dimen/home_below_ttxt" />
                                    </LinearLayout>
                                </RelativeLayout>


                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:weightSum="3">

                                <RelativeLayout
                                    android:id="@+id/totaloil"
                                    android:layout_width="match_parent"
                                    android:layout_height="160dp"
                                    android:layout_weight="1">

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true"
                                        android:gravity="center"
                                        android:orientation="vertical">

                                        <ImageView
                                            android:layout_width="50dp"
                                            android:layout_height="50dp"
                                            android:padding="0dp"
                                            android:src="@drawable/oilb" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="5dp"
                                            android:gravity="center"
                                            android:text="@string/totaloil"
                                            android:textColor="@color/colorPrimaryDark"
                                            android:textSize="@dimen/home_item_txt_size" />
                                    </LinearLayout>
                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/rvSnack"
                                    android:layout_width="match_parent"
                                    android:layout_height="160dp"
                                    android:layout_weight="1">

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true"
                                        android:gravity="center"
                                        android:orientation="vertical">

                                        <ImageView
                                            android:layout_width="50dp"
                                            android:layout_height="50dp"
                                            android:padding="0dp"
                                            android:src="@drawable/acc" />

                                        <TextView
                                            android:id="@+id/myAccount_text"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="5dp"
                                            android:gravity="center"
                                            android:text="My Cars"
                                            android:textColor="@color/colorPrimaryDark"
                                            android:textSize="@dimen/home_item_txt_size" />
                                    </LinearLayout>
                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/info_rv"
                                    android:layout_width="match_parent"
                                    android:layout_height="160dp"
                                    android:layout_weight="1">

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true"
                                        android:gravity="center"

                                        android:orientation="vertical">

                                        <ImageView
                                            android:layout_width="50dp"
                                            android:layout_height="50dp"
                                            android:padding="0dp"
                                            android:src="@drawable/info" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="5dp"
                                            android:gravity="center"
                                            android:text="@string/Info"
                                            android:textColor="@color/colorPrimaryDark"
                                            android:textSize="@dimen/home_item_txt_size" />
                                    </LinearLayout>
                                </RelativeLayout>

                            </LinearLayout>

                        </LinearLayout>
                    </RelativeLayout>

                    <!--Added for Language Tag : Start-->

                    <!--Added for Language Tag :End-->
                </LinearLayout>


            </RelativeLayout>
        </ScrollView>


    </RelativeLayout>
</layout>